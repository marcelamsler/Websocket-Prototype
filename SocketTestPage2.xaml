<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="WebsocketTest.SocketTestPage2">
	<ContentPage.Content>
		<StackLayout>
			<StackLayout.Children>
				<Label x:Name="valueLabel" Text="Message" FontSize="Large" HorizontalOptions="Center" VerticalOptions="Start" />
				<Label x:Name="debugLabel" Text="Debug" FontSize="Large" HorizontalOptions="Center" VerticalOptions="Start" />
				<Button x:Name="connectButton" Text="Connect" HorizontalOptions="Center" VerticalOptions="Start"/>
				<Button x:Name="sendEventMessageButton" Text="Send Activity Event" HorizontalOptions="Center" VerticalOptions="CenterAndExpand"/>
				<Button x:Name="sendRecordMessageButton" Text="Send Activity Record" HorizontalOptions="Center" VerticalOptions="CenterAndExpand"/>
				<Button x:Name="testMultipleMessagesButton" Text="Test Multiple Messages" HorizontalOptions="Center" VerticalOptions="CenterAndExpand"/>
				<Button x:Name="sendRegister" Text="Send Register Message" HorizontalOptions="Center" VerticalOptions="CenterAndExpand"/>
				<Button x:Name="sendDeregister" Text="Send De-Register Message" HorizontalOptions="Center" VerticalOptions="CenterAndExpand"/>
				<Button x:Name="sendJoin" Text="Send Join Message" HorizontalOptions="Center" VerticalOptions="CenterAndExpand"/>

			</StackLayout.Children>
		</StackLayout>
	</ContentPage.Content>
</ContentPage>